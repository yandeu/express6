{
  "name": "express6",
  "version": "0.1.2",
  "description": "ðŸ… Modern, fast, unopinionated, minimalist web framework.",
  "main": "./lib/express.js",
  "types": "./lib/express.d.ts",
  "type": "commonjs",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm-run-all --parallel dev:*",
    "dev:lib": "tsc -p tsconfig.cjs.json --watch",
    "dev:dev": "tsc -p tsconfig.dev.json --watch",
    "dev:nodemon": "nodemon dev.js",
    "build": "tsc -p tsconfig.cjs.json",
    "build-cjs": "tsc -p tsconfig.cjs.json",
    "format:check": "prettier --check {src,test}/**/*.{js,ts}",
    "format": "prettier --write {src,test}/**/*.{js,ts}",
    "lint": "eslint src --ext .ts,.tsx && eslint test --ext .js && eslint examples --ext .js",
    "lint:fix": "eslint --fix src --ext .ts,.tsx",
    "pretest": "node scripts/testing.mjs main index.js",
    "posttest": "node scripts/testing.mjs main ./lib/express.js",
    "test": "npx nyc --reporter=text mocha --check-leaks",
    "test-ci": "npm run pretest && npx nyc --reporter=lcov mocha --check-leaks && npx codecov && npm run posttest",
    "publish": "npx publish-cli@latest",
    "preReleaseHook": "updatePackageLock build test",
    "prepublishOnly": "npm i && npm run build",
    "updatePackageLock": "npm i --package-lock-only"
  },
  "keywords": [
    "express",
    "framework",
    "web",
    "http",
    "rest",
    "restful",
    "router",
    "app",
    "api"
  ],
  "author": "Yannick Deubel (https://github.com/yandeu)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yandeu/express6.git"
  },
  "bugs": {
    "url": "https://github.com/yandeu/express6/issues"
  },
  "dependencies": {
    "accepts": "^1.3.7",
    "array-flatten": "^3.0.0",
    "content-disposition": "^0.5.3",
    "content-type": "^1.0.4",
    "cookie": "^0.4.0",
    "cookie-signature": "^1.0.6",
    "debug": "^4.3.2",
    "encodeurl": "^1.0.2",
    "escape-html": "^1.0.3",
    "etag": "^1.8.1",
    "finalhandler": "^1.1.2",
    "fresh": "^0.5.2",
    "merge-descriptors": "^1.0.1",
    "methods": "^1.1.2",
    "on-finished": "^2.3.0",
    "parseurl": "^1.3.3",
    "path-to-regexp": "^0.1.7",
    "proxy-addr": "^2.0.5",
    "qs": "^6.7.0",
    "range-parser": "^1.2.1",
    "send": "^0.17.1",
    "serve-static": "^1.14.1",
    "statuses": "^2.0.1",
    "type-is": "^1.6.18",
    "utils-merge": "^1.0.1",
    "vary": "^1.1.2"
  },
  "devDependencies": {
    "@types/accepts": "^1.3.5",
    "@types/content-disposition": "^0.5.4",
    "@types/content-type": "^1.1.5",
    "@types/debug": "^4.1.7",
    "@types/encodeurl": "^1.0.0",
    "@types/escape-html": "^1.0.1",
    "@types/etag": "^1.8.1",
    "@types/fresh": "^0.5.0",
    "@types/methods": "^1.1.1",
    "@types/node": "^16.11.6",
    "@types/parseurl": "^1.3.1",
    "@types/path-is-absolute": "^1.0.0",
    "@types/proxy-addr": "^2.0.0",
    "@types/qs": "^6.9.7",
    "@types/range-parser": "^1.2.4",
    "@types/send": "^0.17.1",
    "@types/type-is": "^1.6.3",
    "@types/utils-merge": "^0.0.28",
    "@yandeu/eslint-config": "^0.0.3",
    "@yandeu/prettier-config": "^0.0.2",
    "after": "^0.8.2",
    "body-parser": "^1.19.0",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "ejs": "^3.1.6",
    "express-session": "^1.17.0",
    "marked": "^4.0.3",
    "method-override": "^3.0.0",
    "mocha": "^9.1.3",
    "morgan": "^1.9.1",
    "multiparty": "^4.2.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "pbkdf2-password": "^1.2.1",
    "should": "^13.2.3",
    "supertest": "^6.1.3",
    "typescript": "^4.4.4",
    "vhost": "^3.0.2"
  },
  "engines": {
    "node": "^14.15 || >=16"
  }
}